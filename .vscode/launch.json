{
  "configurations": [{
      "type": "chrome",
      "request": "attach",
      "name": "Attach to Chrome",
      "port": 9222,
      "webRoot": "${workspaceFolder}"
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome",
      "url": "http://localhost:4200/#",
      "webRoot": "${workspaceFolder}",
      "userDataDir": true,
      "runtimeArgs": [
        "--remote-debugging-port=9222"
      ]
    }
  ]
}
